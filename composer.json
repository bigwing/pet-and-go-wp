{
  "name": "bigwing/pet-and-go-wp",
  "description": "PHP library for interacting with the Pet and Go HTTP API inside of WordPress.",
  "version": "1.0.0",
  "license": "GPL-2.0-or-later",
  "keywords": [
	"wordpress",
	"api",
	"petandgo",
	"http"
  ],
  "authors": [
	{
	  "name": "BigWing",
	  "email": "wordpress@bigwing.com"
	},
	{
	  "name": "Morgan Estes",
	  "email": "mestes@bigwing.com",
	  "role": "Developer"
	}
  ],
  "require": {
	"php": ">=7.2",
	"ext-json": "*",
	"ext-simplexml": "*",
	"psr/log": "^1.1",
	"monolog/monolog": "^2.1",
    "myclabs/php-enum": "^1.7"
  },
  "require-dev": {
	"bigwing/phpcs-composer": "dev-master",
    "coenjacobs/mozart": "^0.5.1",
	"johnpbloch/wordpress-core": ">=5.3",
	"pds/skeleton": "^1.0"
  },
  "autoload": {
	"psr-4": {
	  "BigWing\\PetAndGo\\": [
		"src/"
	  ]
	}
  },
  "config": {
	"preferred-install": "dist"
  },
  "extra": {
	"mozart": {
	  "dep_namespace": "BigWing\\PetAndGo\\Dependencies\\",
	  "dep_directory": "/src/Dependencies/",
	  "classmap_directory": "/classes/dependencies/",
	  "classmap_prefix": "BWPG_",
	  "packages": [
		"monolog/monolog",
		"myclabs/php-enum"
	  ],
	  "delete_vendor_directories": true
	}
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
	"post-install-cmd": [
	  "\"vendor/bin/mozart\" compose",
	  "composer dump-autoload"
	],
	"post-update-cmd": [
	  "\"vendor/bin/mozart\" compose",
	  "composer dump-autoload"
	]
  }
}
